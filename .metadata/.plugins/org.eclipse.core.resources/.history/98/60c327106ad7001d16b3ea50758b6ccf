package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 선언문
		String nationKorea = "대한민국";
		String nationEnglish = "Korea";
		String fruits = "\nApple \t사과 \nBanana \t바나나";
		String star = "**";

		// 출력문
		System.out.println(star + nationKorea + star + fruits);
		System.out.println();
		System.out.println(star + nationEnglish + star + fruits);
		
	}

}
